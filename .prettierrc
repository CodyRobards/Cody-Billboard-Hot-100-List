{
  "$schema": "https://json.schemastore.org/prettierrc",
  "plugins": ["prettier-plugin-astro"],
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro",
        "singleQuote": false
      }
    }
  ]
}
