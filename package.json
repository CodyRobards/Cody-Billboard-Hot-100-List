{
  "name": "cody-billboard-hot-100-list",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "npm run validate:album-art && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint \"src/**/*.{astro,ts,tsx,js,jsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"**/*.{astro,ts,tsx,js,jsx,json,css}\"",
    "format:fix": "prettier --write \"**/*.{astro,ts,tsx,js,jsx,json,css}\"",
    "prepare": "husky",
    "test": "vitest run",
    "fetch:album-art": "tsx scripts/fetch-album-art.ts",
    "resize:album-art": "node scripts/resize-album-art.js",
    "validate:album-art": "tsx scripts/validate-album-art.ts"
  },
  "dependencies": {
    "@astrojs/critters": "npm:astro-critters@^2.2.1",
    "@astrojs/mdx": "^4.3.6",
    "astro": "^5.14.1",
    "gray-matter": "^4.0.3",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@types/node": "^24.6.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "webpagetest": "^0.7.6",
    "yaml": "^2.8.1"
  },
  "lint-staged": {
    "*.{astro,ts,tsx,js,jsx,json,css}": [
      "prettier --write"
    ],
    "*.{astro,ts,tsx,js,jsx}": [
      "eslint --max-warnings=0"
    ]
  }
}
