---
/* eslint-disable astro/no-deprecated-astro-resolve */
const title = "Astro Starter";
const canResolveScript = typeof Astro.resolve === "function";
const resolvedScript = canResolveScript
  ? Astro.resolve("../scripts/prefetch-navigation.ts")
  : undefined;
const scriptSrc = resolvedScript ?? "../scripts/prefetch-navigation.ts";
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>{title}</title>
  </head>
  <body>
    <main id="content-root">
      <h1>{title}</h1>
      <p>Get started by editing <code>src/pages/index.astro</code>.</p>
    </main>
    <script type="module" src={scriptSrc}></script>
  </body>
</html>
